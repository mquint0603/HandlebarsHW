<h1>Burgers!</h1>
{{!-- <div> --}}
    <h2>Uneaten burgers will go here</h2>
<div class="uneaten">
  {{#each burgers}}
    {{#unless devoured}}
      <p class="burgerP">{{id}}. {{burger_name}} <button class="eat-btn" id="{{id}}">Eat it!</button></p>
      
    {{/unless}}
  {{/each}}

</div>


{{!-- </div> --}}

<div class="devoured">
    <h2>Eaten will go here</h2>
    {{#each burgers}}
    {{#if devoured}}
      <p class="burgerP">{{id}}. {{burger_name}} </p>
      
    {{/if}}
  {{/each}}
</div>

<h2>Add a Burger</h2>
<form class="create-form">

  <div class="form-group">
    <label for="bname">Burger Name:</label>
    <input type="text" id="bname" name="name">
  </div>

  <button type="submit">Add Burger</button>
</form>


 